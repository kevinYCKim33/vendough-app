{{#contacts}}
  <a href = "/users/{{id}}/dealings" class="list-group-item">
    <div class="froggy">
      <img src={{avatar_url}} width="60" height="60" style="border-radius:50%">
      <div style="padding-left: 17px;">
        <h5>{{name}}</h5>
        <p style="color: rgb(97,148,203)">{{email}}</p>
      </div>
    </div>
  </a>
{{/contacts}}
